{
	"name": "campus-navigation-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint . --ignore-pattern \"coverage/*\"",
		"preview": "vite preview",
		"test": "vitest run --coverage=false --passWithNoTests",
		"test:watch": "vitest --coverage=false --passWithNoTests",
		"coverage": "rimraf coverage && npx vitest --coverage --passWithNoTests run",
		"coverage:watch": "rimraf coverage && npx vitest --coverage --passWithNoTests"
	},
	"dependencies": {
		"geojson": "^0.5.0",
		"leaflet": "^1.9.4",
		"leaflet-routing-machine": "^3.2.12",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-leaflet": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@types/leaflet": "^1.9.21",
		"@types/node": "^24.6.0",
		"@types/react": "^19.1.16",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.4",
		"@vitest/coverage-v8": "^3.2.4",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"eslint": "^9.36.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.22",
		"globals": "^16.4.0",
		"jsdom": "^27.0.0",
		"rimraf": "^6.0.1",
		"sass": "^1.93.2",
		"sass-embedded": "^1.93.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.45.0",
		"vite": "^7.1.11",
		"vitest": "^3.2.4"
	}
}
